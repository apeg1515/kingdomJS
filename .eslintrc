{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "[eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            2,
            "off"
        ],
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "func-names": 0,
        "no-var": 0,
        "func-style": 0,
        "comma-dangle": 0,
        "valid-jsdoc": 0,
        "vars-on-top": 0,
        "complexity": [2, 8],
        "max-depth": [2, 6],
        "consistent-return": 0,
        "id-match": 0,
        "import/newline-after-import": 0,
        "strict": 0,
        "object-shorthand": 0,
        "prefer-arrow-callback": 0,
        "prefer-template": 0,
        "babel/object-shorthand": 0
    }
}
